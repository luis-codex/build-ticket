---
import Layout from '../layouts/Layout.astro';
import Ticket from '../components/Ticket.astro'
import background from '../assets/background.svg';
---

<Layout>
	<Ticket />
	<img id="background" src={background.src} alt="" fetchpriority="high" />
</Layout>

<style>
	#background {
		position: fixed;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		z-index: -1;
		pointer-events: none;
		filter: blur(100px);
	}
	</style>